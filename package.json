{
  "name": "image-similarity-grouper",
  "version": "1.0.0",
  "description": "Script para agrupar imagens de joias por similaridade usando embeddings",
  "type": "module",
  "main": "match-images/match-images.js",
  "scripts": {
    "match": "tsc && node dist/match-images/match-images.js",
    "rename": "tsc && node dist/rename-images/rename-images.js",
    "match-move": "tsc && node dist/rename-images/match-and-move.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "add-js-ext": "ts-node add-js-extensions.ts"
  },
  "keywords": [
    "image",
    "similarity",
    "embeddings",
    "openai",
    "jewelry"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.67.3",
    "sharp": "^0.34.4",
    "sqlite3": "^5.1.7"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.0",
    "@types/sharp": "^0.32.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
