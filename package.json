{
  "name": "image-similarity-grouper",
  "version": "1.0.0",
  "description": "Script para agrupar imagens de joias por similaridade usando embeddings",
  "type": "module",
  "main": "match-images/match-images.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "add-js-ext": "ts-node add-js-extensions.ts",
    "find-duplicates-input": "tsc && node dist/rename-images/find-duplicates-input.js",
    "categorize-images": "tsc && node dist/rename-images/categorize-images.js",
    "categorize-images:clear": "tsc && node dist/rename-images/categorize-images.js --clear-cache"
  },
  "keywords": [
    "image",
    "similarity",
    "embeddings",
    "openai",
    "jewelry"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "openai": "^4.67.3",
    "prisma": "^7.2.0",
    "sharp": "^0.34.4",
    "sqlite3": "^5.1.7",
    "uploadthing": "^7.7.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.0",
    "@types/sharp": "^0.32.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
